  "scripts": {
    "sprites": "npm-run-all --parallel sprites:*",
    "sprites:png": "spritesmith --rc .spritesmith.js",
    "clean": "npm-run-all clean:*",
    "clean:dist": "rimraf dist",
    "copy": "npm-run-all copy:*",
    "copy:fonts": "copyfiles -f 'src/fonts/**/*.{otf,ttf,woff,woff2}' 'dist/assets/'",
    "clean-and-copy": "npm-run-all clean copy",
    "prestart": "npm-run-all --parallel clean-and-copy sprites",
    "start": "webpack-dev-server --open --env.dev",
    "prebuild": "npm-run-all --parallel clean-and-copy sprites",
    "prebuild:prod": "npm-run-all --parallel clean-and-copy sprites",
    "build": "webpack --env.dev --progress",
    "build:prod": "webpack --env.prod -p",
    "debug": "webpack -d --env.dev --display-error-details"
  },
  "engines": {
    "node": ">=4.2.6"
  },
  "private": true
