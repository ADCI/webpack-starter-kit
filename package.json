{
  "name": "webpack-starter-kit",
  "version": "1.0.0-beta",
  "description": "Webpack boilerplate config",
  "keywords": [
    "webpack",
    "front-end build"
  ],
  "homepage": "https://github.com/ADCI/webpack-starter-kit",
  "bugs": {
    "url": "https://github.com/ADCI/webpack-starter-kit/issues",
    "email": "guryanov.artyom@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ADCI/webpack-starter-kit"
  },
  "main": "webpack.config.babel.js",
  "engines": {
    "node": ">=4.2.6"
  },
  "private": true,
  "scripts": {
    "sprites": "npm-run-all --parallel sprites:*",
    "sprites:png": "spritesmith --rc .spritesmith.js",
    "clean": "npm-run-all clean:*",
    "clean:dist": "rimraf dist",
    "prestart": "npm-run-all --parallel clean sprites",
    "start": "webpack --env.dev --progress",
    "start:with-server": "webpack-dev-server --open --env.dev",
    "prebuild": "npm-run-all --parallel clean sprites",
    "prebuild:prod": "npm-run-all --parallel clean sprites",
    "build": "webpack --env.build --progress",
    "build:prod": "webpack --env.prod -p",
    "debug": "webpack -d --env.dev --display-error-details"
  },
  "dependencies": {
    "normalize.css": "^6.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "bundle-loader": "^0.5.4",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.10.0",
    "doiuse": "^2.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "imports-loader": "^0.7.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.1",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-reporter": "^3.0.0",
    "pug-html-loader": "^1.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.0",
    "spritesmith-cli": "^1.1.2",
    "style-loader": "^0.17.0",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-scss": "^1.4.4",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
